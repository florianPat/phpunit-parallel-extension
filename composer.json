{
    "name": "florianpat/phpunit-parallel-extension",
    "description": "Run phpunit tests in parallel",
    "type": "library",
    "keywords": [
        "phpunit",
        "xunit",
        "testing",
        "parallel",
        "multithread"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Florian Patruck",
            "email": "florian.patruck@gmx.de"
        }
    ],
    "support": {
        "issues": "https://github.com/florianPat/phpunit-parallel-extension/issues"
    },
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-parallel": "*",
        "phpunit/phpunit": "10.0.19",
        "symfony/finder": "^6.2"
    },
    "config": {
        "platform": {
            "php": "8.1.0"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "files": [
            "src/ParallelBootstrap.php"
        ],
        "psr-4": {
            "ParallelExtension\\": "src/"
        }
    }
}
